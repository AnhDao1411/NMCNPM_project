<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:background="@drawable/home_screen_img"
    tools:context=".HomeScreen">

    <Button
        android:id="@+id/profile_btn"
        android:layout_width="51dp"
        android:layout_height="43dp"
        android:background="@drawable/profile_button"
        app:layout_constraintBottom_toTopOf="@+id/guideline2"
        app:layout_constraintEnd_toStartOf="@+id/edit_timkiem"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent" />

    <Button
        android:id="@+id/search_btn"
        android:layout_width="51dp"
        android:layout_height="43dp"
        android:background="@drawable/find_button"
        app:layout_constraintBottom_toTopOf="@+id/guideline2"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toEndOf="@+id/edit_timkiem"
        app:layout_constraintTop_toTopOf="parent" />

    <Button
        android:id="@+id/home_btn"
        android:layout_width="51dp"
        android:layout_height="43dp"
        android:layout_marginBottom="16dp"
        android:background="@drawable/home_button"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent" />

    <EditText
        android:id="@+id/edit_timkiem"
        style="@style/edit_text"
        android:layout_width="262dp"
        android:layout_height="45dp"
        android:background="@drawable/round_button_green"
        android:backgroundTint="#2F4634"
        android:importantForAutofill="no"
        app:layout_constraintBottom_toTopOf="@+id/guideline2"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        tools:ignore="LabelFor" />

    <Button
        android:id="@+id/btn_quocgia"
        android:layout_width="115dp"
        android:layout_height="wrap_content"
        android:contentDescription="@string/btn_QG"
        android:text="@string/btn_QG"
        app:layout_constraintLeft_toLeftOf="parent"
        app:layout_constraintStart_toEndOf="@+id/btn_theloai"
        app:layout_constraintTop_toTopOf="@+id/guideline2"
         />

    <Button
        android:id="@+id/btn_theloai"
        android:layout_width="115dp"
        android:layout_height="wrap_content"
        android:contentDescription="@string/btn_TL"
        android:text="@string/btn_TL"
        app:layout_constraintLeft_toLeftOf="parent"
        app:layout_constraintTop_toTopOf="@+id/guideline2" />

    <Button
        android:id="@+id/btn_option"
        android:layout_width="87dp"
        android:layout_height="wrap_content"
        android:contentDescription="@string/btn_moreOp"
        android:text="@string/btn_moreOp"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="1.0"
        app:layout_constraintStart_toEndOf="@+id/btn_nam"
        app:layout_constraintTop_toTopOf="@+id/guideline2" />

    <Button
        android:id="@+id/btn_nam"
        android:layout_width="115dp"
        android:layout_height="wrap_content"
        android:contentDescription="@string/btn_N"
        android:text="@string/btn_N"
        app:layout_constraintLeft_toLeftOf="parent"
        app:layout_constraintStart_toEndOf="@+id/btn_quocgia"
        app:layout_constraintTop_toTopOf="@+id/guideline2" />

    <androidx.constraintlayout.widget.Guideline
        android:id="@+id/guideline2"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:orientation="horizontal"
        app:layout_constraintGuide_begin="77dp" />


</androidx.constraintlayout.widget.ConstraintLayout>
